<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="projector.controller.FirstSetupController">
    <center>
        <HBox alignment="CENTER" spacing="12.0">
            <Button fx:id="copyDataButton" text="Copy data from previous version" onAction="#onCopyDataFromPreviousVersion"/>
            <Button fx:id="startAsNewButton" text="Start as new" onAction="#onStartAsNew"/>
        </HBox>
    </center>
</BorderPane>